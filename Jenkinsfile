pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo 'hello this is sowji'
      }
    }

    stage('test') {
      steps {
        echo 'this is bhaskar'
        sh 'uname -a'
      }
    }

    stage('deploy') {
      steps {
        sh 'echo "hello this is sowjanya and im learning jenkins'
      }
    }

  }
}